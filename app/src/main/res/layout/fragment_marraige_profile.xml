<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clickable="true"
    android:focusable="true"
    android:background="@color/white"
    tools:context="com.rcloud.business.fragment.MarraigeProfileFragment">


    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:background="#8ACDBB86"
        android:layout_height="wrap_content">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/line1"
            android:padding="10dp"
            android:background="@drawable/border2"
            android:orientation="vertical">



            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:text="Bio"
                    android:gravity="center"
                    android:textSize="8dp"
                    android:padding="8dp"
                    android:background="@drawable/border1"
                    android:fontFamily="@font/roboto_slab"
                    android:textColor="@color/white"
                    />
                <EditText
                    android:id="@+id/bio"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:textSize="14sp"
                    android:hint="Bio..."
                    android:padding="8dp"
                    android:textColorHint="@color/white"
                    android:background="@drawable/border"
                    android:fontFamily="@font/roboto_slab"
                    android:textColor="@color/white"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:layout_marginTop="5dp"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:text="Religion"
                    android:background="@drawable/border1"
                    android:gravity="center"
                    android:padding="8dp"
                    android:textSize="8sp"
                    android:fontFamily="@font/roboto_slab"
                    android:textColor="@color/white"
                    />
                <EditText
                    android:id="@+id/religion"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:textSize="14sp"
                    android:hint="Religion..."
                    android:padding="8dp"
                    android:textColorHint="@color/white"
                    android:background="@drawable/border"
                    android:fontFamily="@font/roboto_slab"
                    android:textColor="@color/white"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:layout_marginTop="5dp"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:text="Caste"
                    android:textSize="8sp"
                    android:background="@drawable/border1"
                    android:gravity="center"
                    android:padding="8dp"
                    android:fontFamily="@font/roboto_slab"
                    android:textColor="@color/white"
                    />
                <EditText
                    android:id="@+id/caste"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:background="@drawable/border"
                    android:textSize="14sp"
                    android:hint="Caste..."
                    android:padding="8dp"
                    android:textColorHint="@color/white"
                    android:fontFamily="@font/roboto_slab"
                    android:textColor="@color/white"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:layout_marginTop="5dp"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:text="Gothra"
                    android:padding="8dp"
                    android:gravity="center"
                    android:textSize="8sp"
                    android:fontFamily="@font/roboto_slab"
                    android:textColor="@color/white"
                    android:background="@drawable/border1"
                    />
                <EditText
                    android:id="@+id/gothra"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:textSize="14sp"
                    android:hint="Gothra..."
                    android:padding="8dp"
                    android:textColorHint="@color/white"
                    android:background="@drawable/border"
                    android:fontFamily="@font/roboto_slab"
                    android:textColor="@color/white"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:layout_marginTop="5dp"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:text="Age"
                    android:textSize="8sp"
                    android:fontFamily="@font/roboto_slab"
                    android:textColor="@color/white"
                    android:gravity="center"
                    android:background="@drawable/border1"
                    android:padding="8dp"
                    />
                <EditText
                    android:id="@+id/age"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:textSize="14sp"
                    android:background="@drawable/border"
                    android:hint="Age..."
                    android:padding="8dp"
                    android:textColorHint="@color/white"
                    android:fontFamily="@font/roboto_slab"
                    android:textColor="@color/white"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:layout_marginTop="5dp"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:text="Height"
                    android:background="@drawable/border1"
                    android:textSize="8sp"
                    android:padding="8dp"
                    android:gravity="center"
                    android:fontFamily="@font/roboto_slab"
                    android:textColor="@color/white"
                    />
                <EditText
                    android:id="@+id/height"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:textSize="14sp"
                    android:hint="Height..."
                    android:padding="8dp"
                    android:textColorHint="@color/white"
                    android:background="@drawable/border"
                    android:fontFamily="@font/roboto_slab"
                    android:textColor="@color/white"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:layout_marginTop="5dp"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:text="Country"
                    android:gravity="center"
                    android:background="@drawable/border1"
                    android:textSize="8dp"
                    android:padding="8dp"
                    android:fontFamily="@font/roboto_slab"
                    android:textColor="@color/white"
                    />
                <EditText
                    android:id="@+id/country"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:background="@drawable/border"
                    android:textSize="14sp"
                    android:hint="Country..."
                    android:padding="8dp"
                    android:textColorHint="@color/white"
                    android:fontFamily="@font/roboto_slab"
                    android:textColor="@color/white"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:layout_marginTop="5dp"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="Marital Status"
                    android:gravity="center"
                    android:background="@drawable/border1"
                    android:textSize="8dp"
                    android:padding="8dp"
                    android:fontFamily="@font/roboto_slab"
                    android:textColor="@color/white"
                    />
                <LinearLayout
                    android:layout_width="match_parent"
                    android:background="@drawable/button_round"
                    android:layout_marginBottom="8dp"
                    android:layout_marginEnd="8dp"
                    android:layout_marginStart="8dp"
                    android:layout_height="wrap_content">
                    <Spinner
                        android:id="@+id/spinner_res"
                        android:layout_width="match_parent"
                        android:layout_height="45dp"
                        />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:layout_marginTop="5dp"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:text="Manglik"
                    android:textSize="8dp"
                    android:padding="8dp"
                    android:background="@drawable/border1"
                    android:gravity="center"
                    android:fontFamily="@font/roboto_slab"
                    android:textColor="@color/white"
                    />
                <EditText
                    android:id="@+id/maglik"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:textSize="14sp"
                    android:background="@drawable/border"
                    android:hint="Manglik..."
                    android:padding="8dp"
                    android:textColorHint="@color/white"
                    android:fontFamily="@font/roboto_slab"
                    android:textColor="@color/white"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:layout_marginTop="5dp"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="Highest Degree"
                    android:textSize="8dp"
                    android:padding="8dp"
                    android:gravity="center"
                    android:background="@drawable/border1"
                    android:fontFamily="@font/roboto_slab"
                    android:textColor="@color/white"
                    />
                <EditText
                    android:id="@+id/highdegree"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:textSize="14sp"
                    android:background="@drawable/border"
                    android:hint="Highest degree..."
                    android:padding="8dp"
                    android:textColorHint="@color/white"
                    android:fontFamily="@font/roboto_slab"
                    android:textColor="@color/white"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:layout_marginTop="5dp"
                android:layout_height="wrap_content">
                <TextView
                    android:layout_width="70dp"
                    android:layout_height="wrap_content"
                    android:text="Occupation"
                    android:textSize="8sp"
                    android:gravity="center"
                    android:padding="8dp"
                    android:background="@drawable/border1"
                    android:fontFamily="@font/roboto_slab"
                    android:textColor="@color/white"
                    />
                <EditText
                    android:id="@+id/occuption"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:background="@drawable/border"
                    android:textSize="14sp"
                    android:hint="Occupation..."
                    android:padding="8dp"
                    android:textColorHint="@color/white"
                    android:fontFamily="@font/roboto_slab"
                    android:textColor="@color/white"
                    />
            </LinearLayout>


        </LinearLayout>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="60dp"/>

    </LinearLayout>


</androidx.core.widget.NestedScrollView>